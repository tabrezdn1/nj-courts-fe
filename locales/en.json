    {
      "label": "Arrest Details",
      "value": "arrest-information",
      "desc": "Now, let's discuss the details of the arrest in question.",
      "list": [],
      "stepper": [
        {
          "title": "Date of Arrest",
          "subtitle": "The specific date of the arrest is crucial for our records and the expungement process. Do you recall when it occurred?",
          "icon": "CalendarIcon",
          "fields": [
            {
              "id": "arrest_information_date",
              "type": "date",
              "label": "Date of Arrest",
              "sub_label": "On what date did the arrest take place?",
              "placeholder": "MM/DD/YYYY",
              "validation": {
                "required": true
              }
            }
          ]
        },
        {
          "title": "Jurisdiction of Arrest",
          "subtitle": "To determine the relevant court authority, we need to know where the arrest occurred. Can you provide that information?",
          "icon": "MapPinIcon",
          "fields": [
            {
              "id": "arrest_information_jurisdiction",
              "type": "input",
              "label": "Jurisdiction",
              "sub_label": "In which municipality or jurisdiction did the arrest take place?",
              "placeholder": "Municipality/Jurisdiction",
              "validation": {
                "required": true
              }
            }
          ]
        },
        {
          "title": "Offense Details",
          "subtitle": "To assess your eligibility for expungement, we need specific information about the charges. Could you provide those details?",
          "icon": "ClipboardDocumentListIcon",
          "fields": [
            {
              "id": "arrest_information_offense_name",
              "type": "input",
              "label": "Offense(s) Charged",
              "sub_label": "What offense(s) were you charged with?",
              "placeholder": "Offense Name(s)",
              "validation": {
                "required": true
              }
            },
            {
              "id": "arrest_information_statute_code",
              "type": "input",
              "label": "Statute Code",
              "sub_label": "Do you know the NJSA Statute Code for the primary offense?",
              "placeholder": "NJSA Statute Code",
              "validation": {
                "required": true
              }
            },
            {
              "id": "arrest_information_docket_number",
              "type": "input",
              "label": "Original Case Number",
              "sub_label": "What was the original Indictment/Accusation/Summons/Warrant/Complaint/Docket number?",
              "placeholder": "Case Number",
              "validation": {
                "required": true
              }
            }
          ]
        }
      ]
    },
    {
      "label": "Case Outcome",
      "value": "case-disposition",
      "desc": "Let's discuss how your case was resolved.",
      "list": [],
      "conditionalTabs": {
        "field_id": "case_disposition_status",
        "subTabs": {
          "Yes": [
            {
              "type": "remove",
              "tab": "conviction-information"
            }
          ],
          "No": [
            {
              "type": "add",
              "tab": "conviction-information",
              "index": 3
            }
          ]
        }
      },
      "stepper": [
        {
          "title": "Case Dismissal Status",
          "subtitle": "We need to know if the charges against you were dismissed. Can you provide this information?",
          "icon": "CheckCircleIcon",
          "fields": [
            {
              "id": "case_disposition_status",
              "type": "radio",
              "label": "Dismissal Status",
              "validation": {
                "required": true
              },
              "sub_label": "Were the charges against you dismissed?",
              "name": "dismissalStatus",
              "options": ["Yes", "No"],
              "subFields": {
                "Yes": [
                  {
                    "id": "case_disposition_conditional",
                    "type": "radio",
                    "label": "Conditional Dismissal",
                    "sub_label": "Was the dismissal conditional?",
                    "name": "conditionalDismissal",
                    "validation": {
                      "required": true
                    },
                    "options": ["Yes", "No"],
                    "subFields": {
                      "Yes": [
                        {
                          "id": "case_disposition_programs",
                          "type": "select",
                          "label": "Dismissal Program",
                          "sub_label": "Under which program was your case dismissed?",
                          "name": "dismissalProgram",
                          "validation": {
                            "required": true
                          },
                          "options": [
                            "Conditional Discharge",
                            "Pretrial Intervention",
                            "Juvenile Conference Committee",
                            "Intake Service Conference",
                            "Deferred Disposition"
                          ]
                        }
                      ]
                    }
                  },
                  {
                    "id": "case_disposition_court",
                    "type": "input",
                    "label": "Court of Dismissal",
                    "sub_label": "Which court dismissed the charges?",
                    "placeholder": "Court Name",
                    "validation": {
                      "required": true
                    }
                  },
                  {
                    "id": "case_disposition_date",
                    "type": "date",
                    "label": "Date of Dismissal",
                    "sub_label": "When were the charges dismissed?",
                    "placeholder": "MM/DD/YYYY",
                    "validation": {
                      "required": true
                    }
                  }
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "label": "Conviction Details",
      "value": "conviction-information",
      "desc": "If applicable, we need to discuss any convictions related to this case.",
      "list": [],
